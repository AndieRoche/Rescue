{
  "name": "foster-dog-fieldtrip-app",
  "version": "1.0.0",
  "description": "Foster Dog Field Trip App for Texas Great Pyrenees Rescue",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": ["foster", "dog", "rescue", "fieldtrip"],
  "author": "Texas Great Pyrenees Rescue",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "googleapis": "^128.0.0",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
